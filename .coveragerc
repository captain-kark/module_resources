[run]
branch = 1
omit =
     .venv
     */tests/*
     */__pycache__/*

[report]
precision = 2
fail_under = 10.00
# Regexes for lines to exclude from consideration.
# See: https://coverage.readthedocs.io/en/coverage-4.5.2/excluding.html#advanced-exclusion
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
